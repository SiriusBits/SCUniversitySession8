name: Script
key: script
content-length: 400

$targetItem = get-item .

$targetItem.__Renderings = $sourceItem.__Renderings;
Write-Host "Renderings pasted from '$($sourceItem.ProviderPath)' to '$($targetItem.ProviderPath)'"

if ($sourceItem.PSObject.Properties.Match("__Final Renderings").Count)
{
    $targetItem."__Final Renderings" = $sourceItem."__Final Renderings";
    Write-Host "Found and copied '__Final Renderings' as well"
}

